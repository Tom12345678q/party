<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

  
  
  <!-- 指定Mybatis使用log4j -->
  <settings>
	 <setting name="logImpl" value="LOG4J"/>
  </settings>
  
  <typeAliases>
    <!-- 设置别名 -->
  	<typeAlias type="cn.edu.hist.party.entity.TbUser" alias="User"/>
  	<typeAlias type="cn.edu.hist.party.entity.TbUserRole" alias="UserRole"/>
  	<typeAlias type="cn.edu.hist.party.entity.TbRoles" alias="Roles"/>
  	<typeAlias type="cn.edu.hist.party.entity.TbIndexImage" alias="IndexImage"/>
  	<typeAlias type="cn.edu.hist.party.entity.TbPayLog" alias="PayLog"/>
  	<typeAlias type="cn.edu.hist.party.entity.TbWatchVideoLog" alias="WatchVideoLog"/>
  	<typeAlias type="java.lang.Integer" alias="int"/>
  	<typeAlias type="java.lang.String" alias="String"/>
  	<typeAlias type="java.util.HashSet" alias="HashSet"/>
  	<typeAlias type="java.util.Map" alias="Map"/>
  	<typeAlias type="java.util.List" alias="List"/>
  </typeAliases>
  
  

  
</configuration>
